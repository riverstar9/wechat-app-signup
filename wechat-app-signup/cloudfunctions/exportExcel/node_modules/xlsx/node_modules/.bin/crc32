#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_crc-32@1.2.0@crc-32/bin/crc32.njs" "$@"
  ret=$?
else 
  node  "$basedir/../../../_crc-32@1.2.0@crc-32/bin/crc32.njs" "$@"
  ret=$?
fi
exit $ret
